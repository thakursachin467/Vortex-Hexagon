{{#if errors}}
{{#each errors}}
<div class="alert alert-danger alert-dismissible fade show">
    {{text}}
</div>
{{/each}}
{{else}}

{{/if}}

{{#if error}}
<div class="alert alert-danger alert-dismissible fade show" role="alert">
  {{error}}
</div>
{{else}}

{{/if}}


{{#if error_msg}}
<div class="alert alert-danger alert-dismissible fade show" role="alert">
  <strong>{{error_msg}}</strong>
</div>
{{else}}

{{/if}}
